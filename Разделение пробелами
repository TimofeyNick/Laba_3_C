#include <iostream>
#include <string>

int main()
{
    std:: string s;
    std:: getline(std::cin, s);
    int len_1;
    len_1 = s.length();

    int pos = 0;
    for(int i = 0; i < len_1; i++)
    {
       std:: cout << s[pos];
       if (pos > 1)
       {
           if ((pos+1)%5 == 0)
           {
               std:: cout << ' ';
           }
       }

       pos = pos + 1;
   }

}
